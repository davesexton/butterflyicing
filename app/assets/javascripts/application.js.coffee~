swapImages(divName) ->
  alert('hi')
  img = divName + ' .imgActive'
  active = $(img)
  if $(img).next().length > 0
    next = $(img).next()
  else
    next = $(divName + ' img:first')
    active.fadeOut( ->
      active.removeClass('imgActive')
      next.fadeIn('slow').addClass('imgActive')
    )
